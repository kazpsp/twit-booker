<!DOCTYPE html>
<html>
<head>
  <title>TwitBookr</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div id="flash">
	 	<% flash.each do |key,value| %>
	 		<p><%= value %></p>
	 	<%end%>
 	</div>
<div id="wrapper">
<div id="menu">
	 <div id="innermenu"><%= link_to "Logout", root_path %></div>	 
</div>
<div id="content">
<%= yield %>
</div>
</div>

    <script src="http://connect.facebook.net/en_US/all.js"></script>
<script>
  FB.init({
    appId: 247969818637768,
    cookie: true,
    xfbml: true,
    oauth: true
  });
</script>
</body>	
</html>
